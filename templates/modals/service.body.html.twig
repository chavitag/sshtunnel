				{"idselect":"ip","data":computers, "url":"/list/computers.json", "modalsize":"modal-1024", "title":"Servidores", "value":"ip", "text":"domain","navfunction":navfunction,"rol":rol,"reloadfnc":"refreshComputers"}
			%}
		</div>
		<div class="form-group col-md-6">
